<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.pyfilter" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>PyFilter</name>
    <description>
        PyFilter Test
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PySoft Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="https://fierce-plateau-89068.herokuapp.com/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="SQLitePlugin" value="SQLitePlugin" />
</widget>
